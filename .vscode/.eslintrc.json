{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "globals": {
        "$": true,
        "jQuery": true,
        "module": true,
        "console": true
    },
    "rules": {
        "no-console": [ "off" ],
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "eqeqeq": "error",
        "no-extra-semi": [ "off" ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        //"one-var":"warn",
        "no-unused-vars": [ "error", { "vars": "local" } ],
        "semi": [
            "error",
            "always"
        ],
        //允许 \x08
        "no-control-regex": "off"
    }
}